package-as: Morphomatic

externals: {}   # libs already embedded

ignore:
  # VCS & tooling
  - .git
  - .github
  - .vscode
  - .idea
  - .luarocks
  - Libs/*/.git
  - .gitignore
  - .editorconfig
  - .luacheckrc
  - .stylua.toml
  - stylua.toml
  - cliff.toml

  # Assets you don't want in the zip
  - screenshots
  - logo.png
  - thumbnail.png
  - LATEST_CHANGELOG.md

  # (optional) top-level docs
  - README.md
  - LICENSE

enable-nolib-creation: no
manual-changelog: CHANGELOG.md

curseforge:
  project-id: 1340910
  changelog: CHANGELOG.md
  changelog-format: markdown

wago:
  project-id: j6jznm6R
  changelog: CHANGELOG.md
  changelog-format: markdown
